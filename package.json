{
  "name": "robot_typescript-go-ai",
  "version": "0.1.0",
  "private": true,
  "description": "9x9 Go AI development environment with TypeScript and TensorFlow.js",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "setup:nvm": "powershell -ExecutionPolicy Bypass -File scripts\\install-nvm-windows.ps1",
    "setup:node20": "powershell -Command \"nvm install 20.17.0; nvm use 20.17.0; node -v\"",
    "train:sgf": "ts-node src/train_sgf.ts",
    "play:ai": "ts-node scripts/play_vs_ai.ts"
  },
  "engines": {
    "node": ">=18 <23"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs-node": "^4.22.0"
  },
  "devDependencies": {
    "@types/node": "^22.8.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
